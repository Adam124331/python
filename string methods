
1)     
---------------------------------------
s="hello"
s.isalpha()

o/p :True

2)   
---------------------------------------

s="python"

s.index("n")

output : 5

3)
---------------------------------------

s="python"
s.capitalize()

output : 'Python'

4)
---------------------------------------

s="python"
s.islower()

o/p : True

5)
---------------------------------------

s="python"
s.isdecimal()

o/p : False

6)
---------------------------------------

s="python is versatile"
s.split()

o/p : ['python', 'is', 'versatile']

7)
---------------------------------------

s=" python "
s.join("what")

o/p: 'w python h python a python t'

8)
---------------------------------------

s=" Python "
s.lower()

o/p : python

9)
---------------------------------------

s=" python "
s.upper()

o/p : ' PYTHON '


10)
---------------------------------------

s=" python "
s.count(' ')

o/p : 2

11)
--------------------------------------

s="hello"
s.strip("h")

o/p : ello

12)
---------------------------------------

s="hello"
s.zfill(7)
o/p: '00hello'
