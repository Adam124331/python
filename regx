# Regex 

# Ex - 1 
import re

str1 = "Build your carrier. Get full success"
x = re.findall("success$",str1)
if x:
    print("Yes, it's matching")
else:
    print("Not matching")
    

# Ex - 2 

str2 = "Build your carrier. Get full success"
x1 = re.findall("full$",str2)
if x1:
    print("Yes, it's matching")
else:
    print("Not matching")

# Ex - 3 

str3 = "Build your carrier. Get full success"
x2 = re.findall("carrie.+r",str3)
if x2:
    print("Yes, it's matching")
else:
    print("Not matching")



inheritance
-----------
# Example for inheritance

class product:
    def __init__(self, name, price, deal_price, ratings):
        self.name = name
        self.price = price
        self.deal_price = deal_price
        self.ratings = ratings 
        
    def display_product_details(self):
        print("Product name: {}".format(self.name))
        print("Price: {}".format(self.price))
        print("Deal_price: {}".format(self.deal_price))
        print("Ratings: {}".format(self.ratings))
        
class electronic_item(product):
    def set_warranty(self, warranty):
        self.warranty=warranty 
        
    def get_warranty(self):
        return self.warranty
        
    def display_electronic_product_details(self):
        self.display_product_details()
        print("warranty {}".format(self.warranty))


x = electronic_item("Laptop", 30000, 26000, 4.6)
x.set_warranty(24)
x.display_electronic_product_details()
